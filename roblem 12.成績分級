#include <stdio.h>

int main() {
   int n;
   scanf("%d", &n);

   int score;
   for (int i = 0; i < n; i++) {
      scanf("%d", &score);

      if (score >= 90 && score <= 100) {
         printf("優等\n");
      } else if (score >= 80) {
         printf("甲等\n");
      } else if (score >= 70) {
         printf("乙等\n");
      } else if (score >= 60) {
         printf("丙等\n");
      } else {
         printf("不及格\n");
      }
   }
}
